{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "rules": {
    "eqeqeq": ["error", "always"],
    "no-console": [
      "error",
      {
        "allow": ["error"]
      }
    ]
  }
}
